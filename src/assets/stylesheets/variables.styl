// Basic
white =      #f4f3ef
gray =       #777
// Backgrounds
bg-light =   white
bg-dark =    gray
// Text
text-light = white
text-dark =  gray

// z-levels
zlevel--100 = -100

// font
sans-serif = "Montserrat", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol" !default;