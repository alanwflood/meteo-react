{
  "name": "Meteo",
  "version": "0.0.1",
  "description": "An app that shows the 5 day forecast for a location",
  "main": "index.js",
  "repository": "git@bitbucket.org:alanwflood/meteo.git",
  "author": "Alan Flood",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run build && node server",
    "build": "NODE_ENV=production webpack --config webpack/prod.config.js",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/dev.config.js",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest --coverage"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "date-fns": "^2.10.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.0",
    "react-motion": "^0.5.2",
    "react-places-autocomplete": "^7.2.1",
    "react-svg-inline": "^2.1.1",
    "react-tabs": "^3.1.0",
    "react-test-renderer": "^16.13.0",
    "tippy.js": "^6.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@svgr/webpack": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/servicesMock.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMock.js",
      "\\.(css|less|scss|styl)$": "<rootDir>/tests/styleMock.js"
    }
  }
}
